package javaPrograms;

public class Practice {

	public static void main(String[] args) {
		primeNumber(50);
	
	
}
	public static boolean isPrime(int num) {
		if(num<=1) {
			return false;
		}
		for(int i=2;i<num;i++) {
			if(num%i == 0) {
				return false;
			}
		}
		return true;
	}
	public static void primeNumber(int num) {
		for(int i =2; i<=num;i++) {
			if(isPrime(i)) {
				System.out.println(i+" ");
			}
		}
	}
}
