public class Practice {

	public static void main(String[] args) {
	int a[]= {1,2,3,4,5,7,8};
	int sum=0;
	int sum1=0;
	for(int i=0;i<a.length;i++) {
		sum=sum+a[i];
	}
	for(int i =0;i<=8;i++) {
		sum1=sum1+i;
	}
	System.out.println("Missing number is "+(sum1-sum));
	
	}
		}

