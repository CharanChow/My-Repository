package javaPrograms;

public class Practice {

	public static void main(String[] args) {
	fact(4);
	}
	public static int factorial(int num) {
		int fact = 1;
		for(int i =1;i<=num;i++) {
			fact = fact*i;
			
		}
		return fact;
	}
	public static int fact(int num) {
		if(num == 0) {
			return 1;
		}
		else {
			return = num * fact(num-1);
		}
	}
}
