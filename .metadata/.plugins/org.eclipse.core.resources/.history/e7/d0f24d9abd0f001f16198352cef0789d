public class Practice {

	public static void main(String[] args) {
	String s = "haran";
	String s1 = "narah";
	isAnnagram(s,s1);
	
	}
	public static void isAnnagram(String s, String s1) {
		char[]c1 =s.toCharArray();
		char[]c2=s1.toCharArray();
		Arrays.sort(c1);
		Arrays.sort(c2);
		if(Arrays.equals(c2, c2)) {
			System.out.println("given strings are annagram");
		}
		else {
			System.out.println("given strings are not ammagram");
		}
	}
		}

