public class Practice {

	public static void main(String[] args) {
		isPalandrom(12121);
		
	}
	public static void isPalandrom(int num) {
		int r=0;
		int sum=0;
		int t = num;
		while(num>0) {
			r=num%10;
			sum = (sum*10)+r;
			num = num/10;
		}
		if( t==sum) {
			System.out.println(num+" given num is palindrom");
		}
		else {
			System.out.println(num + " given number is not palindron");
		}
	}
	
		}

