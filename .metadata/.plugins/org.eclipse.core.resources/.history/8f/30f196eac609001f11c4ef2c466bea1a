package javaPrograms;



public class Practice {

	public static void main(String[] args) {
	int a[]= {12,34,25,64,87,-1};
	int temp,size;
	size = a.length;
	for(int i =0;i<size;i++) {
		for(int j=i+1;j<size;j++) {
			if(a[i]>a[j]) {
				temp = a[i];
				a[j]=a[i];
				a[i]=temp;
			}
		}
	}
	System.out.println(a);
		}
}
