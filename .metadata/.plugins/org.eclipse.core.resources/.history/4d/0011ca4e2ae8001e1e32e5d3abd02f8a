import org.openqa.selenium.chrome.ChromeDriver;
import org.openqa.selenium.firefox.FirefoxDriver;

import io.github.bonigarcia.wdm.WebDriverManager;

public class App {
	public static void main(String args[]) {
		
int a[]= {-11,22,33,78,56};
int largest = a[0];
int  smallest =a[0];
for(int i=1; i<a.length;i++) {
	if(largest < a[i]) {
	largest = a[i];
		
	}
	else if(smallest>a[i]) {
		smallest = a[1];
	}
}
System.out.println("smallest number in given number is: "+smallest);
System.out.println("largest number in given number is: "+largest);
	}
}


	



	


